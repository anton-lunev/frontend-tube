{
  "name": "frontend-tube",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "gh-pages": "gh-pages",
    "start": "ng serve",
    "serve:aot": "ng serve --prod --aot",
    "build": "ng build",
    "build:aot": "ng build --prod --aot",
    "build:aot:sw": "ng build --prod --aot && npm run sw",
    "deploy": "gh-pages -d dist",
    "deploy:prod": "npm run build:aot:sw && npm run deploy",
    "sw": "sw-precache --root=dist --verbose --config=sw-precache-config.js",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^4.0.0",
    "@angular/compiler": "^4.0.0",
    "@angular/core": "^4.0.0",
    "@angular/forms": "^4.0.0",
    "@angular/http": "^4.0.0",
    "@angular/platform-browser": "^4.0.0",
    "@angular/platform-browser-dynamic": "^4.0.0",
    "@angular/router": "^4.0.0",
    "angularfire2": "^2.0.0-beta.8",
    "core-js": "^2.4.1",
    "firebase": "^3.7.5",
    "rxjs": "^5.3.0",
    "zone.js": "^0.8.5"
  },
  "devDependencies": {
    "@angular/cli": "^1.0.0",
    "@angular/compiler-cli": "^4.0.0",
    "@angular/language-service": "^4.0.0",
    "@types/jasmine": "~2.5.46",
    "@types/node": "~7.0.10",
    "codelyzer": "~2.1.1",
    "gh-pages": "^0.12.0",
    "jasmine-core": "~2.5.2",
    "jasmine-spec-reporter": "~3.2.0",
    "karma": "~1.6.0",
    "karma-chrome-launcher": "~2.0.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.0.0",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.1.0",
    "sw-precache": "^5.1.0",
    "ts-node": "~3.0.2",
    "tslint": "~4.5.0",
    "typescript": "~2.2.1"
  }
}
